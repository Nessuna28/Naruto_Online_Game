<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <TextView
            android:id="@+id/tv_jutsusPlayer"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/liste_an_jutsus"
            app:layout_constraintBottom_toBottomOf="@+id/cb_jutsusPlayer"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/cb_jutsusPlayer" />

        <CheckBox
            android:id="@+id/cb_jutsusPlayer"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:text=""
            android:clickable="true"
            app:layout_constraintStart_toEndOf="@+id/tv_jutsusPlayer"
            app:layout_constraintTop_toTopOf="parent"
            android:focusable="true" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>